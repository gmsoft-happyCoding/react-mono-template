{
  "private": true,
  "devDependencies": {
    "lerna": "^3.5.1"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start:app": "stormrage start --package app",
    "start:app-next": "stormrage start --package app --next",
    "test:app": "stormrage test --package app",
    "doc:app": "stormrage doc --package app",
    "bad:app": "stormrage bad --package app",
    "start:c": "stormrage start --package components",
    "dev-build:c": "stormrage devbuild",
    "dev-build:c-next": "stormrage devbuild --next",
    "test:c": "stormrage test --package components",
    "doc:c": "stormrage doc --package components",
    "bad:c": "stormrage bad --package components",
    "make-app": "stormrage make --package app",
    "make-components": "stormrage make --package components",
    "deploy-app": "stormrage deploy {{projectDeployNameApp}} test1 zcj",
    "deploy-components": "stormrage deploy {{projectDeployNameComponents}} test1 zcj",
    "genmeta:c": "stormrage genmeta",
    "genapi": "stormrage genapi"
  },
  "resolutions": {
    "ansi-styles": "^3.2.0"
  }
}
